<IconContainer CssClass="mb-1 p-5" IconSize=IconSize.X04>
    <Icons>
        <IconLayersContainer CssClass="mb-1" FixedWidth=true BackgroundColor="#8B9DC3">
            <Icons>
                <Icon IconStyle=IconStyle.Solid IconName="fa-circle" Color="Tomato" />
                <Icon IconStyle=IconStyle.Solid IconName="fa-times" Inverse=true InverseColor="white" Transform=Transform.Shrink(6) />
            </Icons>
        </IconLayersContainer>

        <IconLayersContainer CssClass="mb-1" FixedWidth=true BackgroundColor="#8B9DC3">
            <Icons>
                <Icon IconStyle=IconStyle.Solid IconName="fa-bookmark" />
                <Icon IconStyle=IconStyle.Solid IconName="fa-heart" Inverse=true InverseColor="Tomato" Transform=Transform.Shrink(10).Up(2) />
            </Icons>
        </IconLayersContainer>

        <IconLayersContainer CssClass="mb-1" FixedWidth=true BackgroundColor="#8B9DC3">
            <Icons>
                <Icon IconStyle=IconStyle.Solid IconName="fa-play"  Inverse=false                       Transform=Transform.Rotate(-90).Grow(4) />
                <Icon IconStyle=IconStyle.Solid IconName="fa-sun"   Inverse=true InverseColor="blue"    Transform=Transform.Shrink(10).Up(2) />
                <Icon IconStyle=IconStyle.Solid IconName="fa-moon"  Inverse=true InverseColor="Tomato"  Transform=Transform.Shrink(11).Down(4.2F).Left(4) />
                <Icon IconStyle=IconStyle.Solid IconName="fa-star"  Inverse=true InverseColor="green"   Transform=Transform.Shrink(11).Down(4.2F).Right(4) />
            </Icons>
        </IconLayersContainer>

        <IconLayersContainer CssClass="mb-1" FixedWidth=true BackgroundColor="#8B9DC3">
            <Icons>
                <Icon IconStyle=IconStyle.Solid IconName="fa-calendar" />
                <TextLayer Inverse=true InverseColor="Mediumslateblue" Transform=Transform.Shrink(8).Down(3) style="font-weight:900;" Text="27" />
            </Icons>
        </IconLayersContainer>

        <IconLayersContainer CssClass="mb-1" FixedWidth=true BackgroundColor="#8B9DC3">
            <Icons>
                <Icon IconStyle=IconStyle.Solid IconName="fa-certificate" />
                <TextLayer Inverse=true InverseColor="Mediumslateblue" Transform=Transform.Shrink(11.5F).Rotate(-30) style="font-weight:900;" Text="NEW" />
            </Icons>
        </IconLayersContainer>

        <IconLayersContainer CssClass="mb-1" FixedWidth=true BackgroundColor="#8B9DC3">
            <Icons>
                <Icon IconStyle=IconStyle.Solid IconName="fa-envelope" />
                <CounterLayer BackgroundColor="Tomato" Text="1,419"/>
            </Icons>
        </IconLayersContainer>
    </Icons>
</IconContainer>
